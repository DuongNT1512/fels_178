<div class="search-form">
  <%= form_tag words_path, remote: true, method: :get do %>
    <%= label_tag :search_word %>
    <div class="fl-div-word row", id="search-word-form" >
      <div class="col-md-4"><%= select_tag :search_word,
        options_for_select(@filter_words), class: "form-control" %>
      </div>
      <div class="col-md-4"><%= text_field_tag :search %></div>
      <div class="col-md-4"><%= submit_tag t "containt.search",
        class: "btn btn-primary" %>
      </div>
    </div>
  <% end %>
</div>
